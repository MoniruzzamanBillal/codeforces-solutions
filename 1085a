#include<bits/stdc++.h>
using namespace std;

int main()
{
    string s;
    cin>>s;
    int len=s.size();

    int fir,i,j;
    fir=(len-1)/2;
    j=fir-1;
    i=fir+1;

    cout<<s[fir];

    while(i<len || j>=0){
        if(i<len){
            cout<<s[i];
        }

        if(j>=0){
            cout<<s[j];
        }

        i++;
        j--;
    }

    return 0;
}
