#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    int i;
    int j;
    for(i=0; i<n; i++)
    {
        cin>>a[i];
    }
    sort(a,a+n);
    int cou=0;

    int p[n];
    j=0;
    for(i=0; i<n; i++)
    {
        if(a[i]==0)
        {
            continue;
        }
        else
        {
            p[j]=a[i];
            j++;
        }
    }
    for(i=1; i<j; i++)
    {
        if(p[i]==p[i-1]){
            cou++;
        }
    }
    cout<<(cou>0?j-cou:j);
    return 0;
}
